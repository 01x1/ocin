if not game:IsLoaded() then
    game.Loaded:Wait()
end

local Games = {
    [2788229376]="DaHood",
    [7213786345]="DaHood",
    [205224386]="HideNSeek",
}

if Games[game.PlaceId] then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/01x1/ocin/main/"..tostring(Games[game.PlaceId])))()
else
    warn("Game not supported.")
end
